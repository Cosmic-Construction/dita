                  <codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:6;129:30" xtrf="file:/C:/Git/dita/specification/langRef/base/subjectRelTable.dita">&lt;subjectdef keyref="windowsOS"/&gt;</codeblock>
