                  <codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:2;105:30" xtrf="file:/C:/Git/dita/specification/langRef/base/subjectRelTable.dita"><i class="+ topic/ph hi-d/i " xtrc="i:1;105:33" xtrf="file:/C:/Git/dita/specification/langRef/base/subjectRelTable.dita">&lt;subjectdef keyref="operatingSystem"&gt;
 &lt;hasRelated keyref="environmentFor"&gt;
  &lt;subjectdef keyref="application"/&gt;
 &lt;/hasRelated&gt;
&lt;/subjectdef&gt;</i></codeblock>
                </entry>
                <entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="1" xtrc="entry:2;111:39" xtrf="file:/C:/Git/dita/specification/langRef/base/subjectRelTable.dita">
                  <codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:3;112:30" xtrf="file:/C:/Git/dita/specification/langRef/base/subjectRelTable.dita"><i class="+ topic/ph hi-d/i " xtrc="i:2;112:33" xtrf="file:/C:/Git/dita/specification/langRef/base/subjectRelTable.dita">&lt;subjectdef keyref="application"/&gt;</i></codeblock>
