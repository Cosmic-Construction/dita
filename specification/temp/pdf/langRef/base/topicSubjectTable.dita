            <stentry class="- topic/stentry " xtrc="stentry:2;131:22" xtrf="file:/C:/Git/dita/specification/langRef/base/topicSubjectTable.dita">goal</stentry>
            <stentry class="- topic/stentry " xtrc="stentry:3;132:22" xtrf="file:/C:/Git/dita/specification/langRef/base/topicSubjectTable.dita">os</stentry>
          </sthead>
          <strow class="- topic/strow " xtrc="strow:1;134:18" xtrf="file:/C:/Git/dita/specification/langRef/base/topicSubjectTable.dita">
            <stentry class="- topic/stentry " xtrc="stentry:4;135:22" xtrf="file:/C:/Git/dita/specification/langRef/base/topicSubjectTable.dita">
              <lines class="- topic/lines " xml:space="preserve" xtrc="lines:1;136:22" xtrf="file:/C:/Git/dita/specification/langRef/base/topicSubjectTable.dita"><filepath class="+ topic/ph sw-d/filepath " xtrc="filepath:5;136:32" xtrf="file:/C:/Git/dita/specification/langRef/base/topicSubjectTable.dita">configure-cron-for-efficiency.dita</filepath>

