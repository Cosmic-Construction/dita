          <dt class="- topic/dt " xtrc="dt:4;46:15" xtrf="file:/C:/Git/dita/specification/langRef/base/draft-comment.dita"><xmlatt class="+ topic/keyword markup-d/markupname xml-d/xmlatt " xtrc="xmlatt:4;46:23" xtrf="file:/C:/Git/dita/specification/langRef/base/draft-comment.dita">disposition</xmlatt></dt>
