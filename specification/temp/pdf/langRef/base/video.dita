        <indexterm class="- topic/indexterm " xtrc="indexterm:2;10:20" xtrf="file:/C:/Git/dita/specification/langRef/base/video.dita">multimedia domain<indexterm class="- topic/indexterm " xtrc="indexterm:3;10:48" xtrf="file:/C:/Git/dita/specification/langRef/base/video.dita">video </indexterm></indexterm>
      </keywords>
