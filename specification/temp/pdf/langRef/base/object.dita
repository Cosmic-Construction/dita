                <indexterm class="- topic/indexterm " xtrc="indexterm:3;13:28" xtrf="file:/C:/Git/dita/specification/langRef/base/object.dita">elements<indexterm class="- topic/indexterm " xtrc="indexterm:4;13:47" xtrf="file:/C:/Git/dita/specification/langRef/base/object.dita">body<indexterm class="- topic/indexterm " xtrc="indexterm:5;13:62" xtrf="file:/C:/Git/dita/specification/langRef/base/object.dita"><xmlelement class="+ topic/keyword markup-d/markupname xml-d/xmlelement " xtrc="xmlelement:4;13:74" xtrf="file:/C:/Git/dita/specification/langRef/base/object.dita">object</xmlelement></indexterm></indexterm></indexterm>
