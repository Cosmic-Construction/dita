        <indexterm class="- topic/indexterm " xtrc="indexterm:2;14:20" xtrf="file:/C:/Git/dita/specification/langRef/base/hazardstatement.dita">hazard statement domain<indexterm class="- topic/indexterm " xtrc="indexterm:3;14:54" xtrf="file:/C:/Git/dita/specification/langRef/base/hazardstatement.dita">hazardstatement</indexterm></indexterm></keywords>
