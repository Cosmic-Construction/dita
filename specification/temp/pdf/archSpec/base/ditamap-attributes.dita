					<li class="- topic/li " id="li_178E6EB1985D464A867DDB89066927F5" xtrc="li:9;256:51" xtrf="file:/C:/Git/dita/specification/archSpec/base/ditamap-attributes.dita"><xmlatt class="+ topic/keyword markup-d/markupname xml-d/xmlatt " xtrc="xmlatt:36;256:59" xtrf="file:/C:/Git/dita/specification/archSpec/base/ditamap-attributes.dita">keyref</xmlatt></li>
					<li class="- topic/li " id="li_393B3EADBBBF45029DD13F74FCB01383" xtrc="li:10;257:51" xtrf="file:/C:/Git/dita/specification/archSpec/base/ditamap-attributes.dita"><xmlatt class="+ topic/keyword markup-d/markupname xml-d/xmlatt " xtrc="xmlatt:37;257:59" xtrf="file:/C:/Git/dita/specification/archSpec/base/ditamap-attributes.dita">scope</xmlatt>
					</li>
