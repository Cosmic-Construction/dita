    <stentry class="- topic/stentry " xtrc="stentry:4;38:14" xtrf="file:/C:/Git/dita/specification/archSpec/base/example-resolution-of-duplicate-key-definitions-multiple-maps.dita"><filepath class="+ topic/ph sw-d/filepath " xtrc="filepath:3;38:24" xtrf="file:/C:/Git/dita/specification/archSpec/base/example-resolution-of-duplicate-key-definitions-multiple-maps.dita">toner-type-a-specs.dita</filepath></stentry>
   </strow>
   <strow class="- topic/strow " xtrc="strow:2;40:11" xtrf="file:/C:/Git/dita/specification/archSpec/base/example-resolution-of-duplicate-key-definitions-multiple-maps.dita">
