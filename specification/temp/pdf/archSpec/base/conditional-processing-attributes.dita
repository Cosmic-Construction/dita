            <indexterm class="- topic/indexterm " xtrc="indexterm:3;14:24" xtrf="file:/C:/Git/dita/specification/archSpec/base/conditional-processing-attributes.dita">conditional processing<indexterm class="- topic/indexterm " xtrc="indexterm:4;14:57" xtrf="file:/C:/Git/dita/specification/archSpec/base/conditional-processing-attributes.dita">attributes</indexterm></indexterm>
            <indexterm class="- topic/indexterm " xtrc="indexterm:5;15:24" xtrf="file:/C:/Git/dita/specification/archSpec/base/conditional-processing-attributes.dita">filtering<indexterm class="- topic/indexterm " xtrc="indexterm:6;15:44" xtrf="file:/C:/Git/dita/specification/archSpec/base/conditional-processing-attributes.dita">attributes</indexterm></indexterm>
            <indexterm class="- topic/indexterm " xtrc="indexterm:7;16:24" xtrf="file:/C:/Git/dita/specification/archSpec/base/conditional-processing-attributes.dita">flagging<indexterm class="- topic/indexterm " xtrc="indexterm:8;16:43" xtrf="file:/C:/Git/dita/specification/archSpec/base/conditional-processing-attributes.dita">attributes</indexterm></indexterm>
