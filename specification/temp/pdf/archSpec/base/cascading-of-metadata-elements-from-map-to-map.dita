                <indexterm class="- topic/indexterm " xtrc="indexterm:1;11:28" xtrf="file:/C:/Git/dita/specification/archSpec/base/cascading-of-metadata-elements-from-map-to-map.dita">cascading<indexterm class="- topic/indexterm " xtrc="indexterm:2;11:48" xtrf="file:/C:/Git/dita/specification/archSpec/base/cascading-of-metadata-elements-from-map-to-map.dita">map-to-map<indexterm class="- topic/indexterm " xtrc="indexterm:3;11:69" xtrf="file:/C:/Git/dita/specification/archSpec/base/cascading-of-metadata-elements-from-map-to-map.dita">metadata
                        elements</indexterm></indexterm></indexterm>
                <indexterm class="- topic/indexterm " xtrc="indexterm:4;13:28" xtrf="file:/C:/Git/dita/specification/archSpec/base/cascading-of-metadata-elements-from-map-to-map.dita">map-to-map cascading<indexterm class="- topic/indexterm " xtrc="indexterm:5;13:59" xtrf="file:/C:/Git/dita/specification/archSpec/base/cascading-of-metadata-elements-from-map-to-map.dita">metadata elements</indexterm></indexterm>
