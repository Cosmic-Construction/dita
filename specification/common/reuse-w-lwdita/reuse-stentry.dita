<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="stentry" xml:lang="en-us">
  <title><xmlelement>stentry</xmlelement></title>
  <shortdesc id="shortdesc" platform="dita lwdita">A simple table entry
    represents a single cell within a simple table.</shortdesc>
  <refbody>
    <section id="attributes">
      <title>Attributes</title>
      <div conkeyref="reuse-attributes/stentry-attr" platform="dita"/>
      <div conkeyref="reuse-lwdita-attributes/stentry" platform="lwdita"/>
      <p conkeyref="reuse-lwdita-attributes/xdita-hdita-additional-attr"
        platform="lwdita"/>
      <div platform="dita lwdita">
        <dl>
          <dlentry conkeyref="reuse-attributes/stentry-colspan"
            platform="dita lwdita">
            <dt/>
            <dd/>
          </dlentry>
          <!--<dlentry conkeyref="reuse-attributes/headers-att" platform="dita lwdita"><dt/><dd/></dlentry>-->
          <dlentry conkeyref="reuse-attributes/stentry-rowspan"
            platform="dita lwdita">
            <dt/>
            <dd/>
          </dlentry>
          <!--<dlentry conkeyref="reuse-attributes/tablescope" platform="dita lwdita"><dt/><dd/></dlentry>-->
        </dl>
      </div>
    </section>
  </refbody>
</reference>
